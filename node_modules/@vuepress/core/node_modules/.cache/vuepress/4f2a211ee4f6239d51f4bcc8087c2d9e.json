p 0\n\n    > h1:first-child\n      display none\n\n      + h2\n        margin-top 0.5rem - $navbarHeight !important\n        padding-top $navbarHeight !important\n\n        @media (max-width $MQMobile)\n          margin-top 0.5rem - $navbarMobileHeight !important\n          padding-top $navbarMobileHeight !important\n</style>\n"]}]}